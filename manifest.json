{
  "manifest_version": 2,
  "name": "liver collection",
  "description": "肝脑涂地",
  "version": "1.1.6",
  "permissions": ["contextMenus", "clipboardRead", "notifications", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://game.granbluefantasy.jp/*",
        "*://gbf.game.mbga.jp/*",
        "*://gbf-tbw.tk/*",
        "*://tbw.346lab.org/*",
        "*://gbf-raidfinder.aikats.us/*"
      ],
      "css": ["assets/contentScript/main.css"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "options_page": "index.html#option",
  "browser_action": {
    "default_icon": "assets/img/54878633_p0.png",
    "default_title": "寻找失去的肝脏"
  },
  "web_accessible_resources": ["inject.js"]
}
